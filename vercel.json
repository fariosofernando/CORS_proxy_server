{
    "version": 2,
    "builds": [
        {
            "src": "lib/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "lib/main.ts"
        }
    ],
    "functions": {
        "lib/main.ts": {
            "maxDuration": 60
        }
    }
}